<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>voteSystem</display-name>
  <servlet>
    <servlet-name>registerServlet</servlet-name>
    <servlet-class>com.Servlet.registerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>com.Servlet.loginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>lookPurchchaseServlet</servlet-name>
    <servlet-class>com.Servlet.lookPurchchaseServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>modifyPasswordServlet</servlet-name>
    <servlet-class>com.Servlet.modifyPasswordServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>modifyRegisterServlet</servlet-name>
    <servlet-class>com.Servlet.modifyRegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>lookBookServlet</servlet-name>
    <servlet-class>com.Servlet.lookBookServlet</servlet-class>
  </servlet>

 





  


  <servlet-mapping>
    <servlet-name>registerServlet</servlet-name>
    <url-pattern>/registerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>lookPurchchaseServlet</servlet-name>
    <url-pattern>/lookPurchchaseServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>modifyPasswordServlet</servlet-name>
    <url-pattern>/modifyPasswordServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>modifyRegisterServlet</servlet-name>
    <url-pattern>/modifyRegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>lookBookServlet</servlet-name>
    <url-pattern>/lookBookServlet</url-pattern>
  </servlet-mapping>
  
  
  
  <welcome-file-list>
    <welcome-file>/login.jsp</welcome-file>
  </welcome-file-list>
</web-app>
